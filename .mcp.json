{
  "mcpServers": {
    "sequential-thinking": {
      "type": "stdio",
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "zen": {
      "command": "sh",
      "args": [
        "-c",
        "exec $(which uvx || echo uvx) --from git+https://github.com/BeehiveInnovations/zen-mcp-server.git pal-mcp-server"
      ]
    },
    "qdrant-local": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@mhalder/qdrant-mcp-server"],
      "env": {
        "TRANSPORT_MODE": "stdio",
        "EMBEDDING_PROVIDER": "openai",
        "EMBEDDING_MODEL": "text-embedding-3-large",
        "QDRANT_URL": "http://localhost:6333"
      }
    }
  }
}